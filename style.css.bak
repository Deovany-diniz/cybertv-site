
body {
    font-family: Arial, sans-serif;
    background-color: #1e1e1e;
    color: #fff;
    margin: 0;
    padding: 0;
    text-align: center;
}
header {
    padding: 20px;
}
.logo {
    max-width: 120px;
}
.plano {
    background-color: #2c2c2c;
    margin: 20px;
    padding: 20px;
    border-radius: 10px;
}
ul {
    list-style: none;
    padding: 0;
}
li {
    padding: 5px 0;
}
.preco {
    font-size: 24px;
    margin: 10px 0;
    color: #ff6600;
}
.botao {
    display: inline-block;
    padding: 15px 25px;
    background-color: #ff4900;
    color: #fff;
    text-decoration: none;
    border-radius: 8px;
    font-weight: bold;
}
